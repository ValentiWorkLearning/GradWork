{
    "configurations": [
        {
            "name": "pca10040/s132",
            "includePath": [
                "${workspaceFolder}/sdk_dependent/config",
                "${env:NRF5_SDK_PATH}/**"
            ],
            "defines": [
                "BOARD_PCA10040",
                "CONFIG_GPIO_AS_PINRESET",
                "FLOAT_ABI_HARD",
                "NRF52",
                "NRF52832_XXAA",
                "NRF52_PAN_74",
                "NRF_SD_BLE_API_VERSION=6",
                "S112",
                "SOFTDEVICE_PRESENT",
                "SWI_DISABLE0",
                "USE_NRFSDK_SIMULATOR",
                "USE_HARDWARE_DISPLAY_BACKEND",
                "USE_BLE_SERVICES",
                "USE_DEVICE_SPECIFIC",
                "LoggerSegger"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "go2sh.cmake-integration",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}